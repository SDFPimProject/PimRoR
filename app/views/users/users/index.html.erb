<% if @users != nil %>
    <% provide(:title, "Nutzer") %>
    <div class="user-wrapper">
      <%= will_paginate @users %>
      <table class="user_table">
        <tr>
          <th>Vorname</th>
          <th>Nachname</th>
          <th colspan="2">Email</th>
        </tr>
        <tr>
          <%= render @users %>
        </tr>
      </table>
      <%= will_paginate @users %>
    </div>
<% end %>